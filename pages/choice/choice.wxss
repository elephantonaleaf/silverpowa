.round-button {
  height: 250rpx;
  width: 250rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}

.choice {
  height: 35vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 20rpx;
}

.container {
  padding: 0px;
  position: relative;
  height: 100vh;
  justify-content: center;
  background:#F6F6F6;
}

.navbar-bottom {
  display:flex;
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 150rpx;
  justify-content: space-around;
}

.navbar-item {
  height: 150rpx;
  display:flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  font-family: muli;
  font-size: 15px;
  color:#2E2926;
}

.navbar-icon {
  height: 60rpx;
  width: 60rpx;
  margin-bottom:15rpx;
}

